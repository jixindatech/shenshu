{
  "roles": ["admin"],
  "admin": [
    {
      "path": "/",
      "children": [{
        "path": "dashboard"
      }],
      "api": [
        "GET:/user/info",
        "POST:/system/user/logout"
      ]
    },
    {
      "path": "/system",
      "children": [
        {
          "path": "user",
          "api": [
            "GET:/system/user",
            "POST:/system/user",
            "PUT:/system/user",
            "GET:/system/user/:id",
            "PUT:/system/user/:id",
            "DELETE:/system/user/:id",
            "PUT:/system/user/password/:id"
          ]
        },
        {
          "path": "config",
          "api": [
            "GET:/system/email",
            "POST:/system/email",
            "PUT:/system/email/:id",
            "GET:/system/ldap",
            "POST:/system/ldap",
            "PUT:/system/ldap/:id"
          ]
        }]
    },
    {
      "path": "external-link",
      "children": [
        {
          "path": "https://github.com/jixindatech/shenshu"
        }
      ]
    }
  ]
}