{
  "roles": ["admin"],
  "admin": [
    {
      "path": "/",
      "children": [{
        "path": "dashboard"
      }],
      "api": [
        "GET:/user/info",
        "POST:/system/user/logout"
      ]
    },
    {
      "path": "/nginx",
      "children": [{
        "path": "site",
        "api": [
          "GET:/nginx/site",
          "POST:/nginx/site",
          "GET:/nginx/site/:id",
          "PUT:/nginx/site/:id",
          "DELETE:/nginx/site/:id"
        ]
      }, {
        "path": "ssl",
        "api": [
          "GET:/nginx/ssl",
          "POST:/nginx/ssl",
          "GET:/nginx/ssl/:id",
          "PUT:/nginx/ssl/:id",
          "DELETE:/nginx/ssl/:id"
        ]
      }, {
        "path": "upstream",
        "api": [
          "GET:/nginx/upstream",
          "POST:/nginx/upstream",
          "GET:/nginx/upstream/:id",
          "PUT:/nginx/upstream/:id",
          "DELETE:/nginx/upstream/:id"
        ]
      }]
    },
    {
      "path": "/shenshu",
      "children": [
        {
          "path": "ip",
          "api": [
            "GET:/shenshu/site/:id/ip",
            "POST:/shenshu/site/:id/ip",
            "PUT:/shenshu/site/ip/:id",
            "GET:/shenshu/site/ip/:id",
            "DELETE:/shenshu/site/ip/:id"
          ]
        },
        {
          "path": "cc",
          "api": [
            "GET:/shenshu/site/:id/cc",
            "POST:/shenshu/site/:id/cc",
            "PUT:/shenshu/site/cc/:id",
            "GET:/shenshu/site/cc/:id",
            "DELETE:/shenshu/site/cc/:id"
          ]
        },
        {
          "path": "rule",
          "children": [
            {
              "path": "group",
              "api": [
                "GET:/shenshu/rulegroup",
                "POST:/shenshu/rulegroup",
                "GET:/shenshu/rulegroup/:id",
                "PUT:/shenshu/rulegroup/:id",
                "DELETE:/shenshu/rulegroup/:id"
              ]
            },
            {
              "path": "item",
              "api": [
                "GET:/shenshu/rulegroup/:id/rule",
                "POST:/shenshu/rulegroup/:id/rule",
                "PUT:/shenshu/rulegroup/rule/:id",
                "GET:/shenshu/rulegroup/rule/:id",
                "DELETE:/shenshu/rulegroup/rule/:id"
              ]
            },
            {
              "path": "batch",
              "api": [
                "GET:/shenshu/rulegroup/:id/rulebatch",
                "POST:/shenshu/rulegroup/:id/rulebatch",
                "PUT:/shenshu/rulegroup/rulebatch/:id",
                "GET:/shenshu/rulegroup/rulebatch/:id",
                "DELETE:/shenshu/rulegroup/rulebatch/:id"
              ]
            }
          ]
        }
      ]
    },
    {
      "path": "/system",
      "children": [
        {
          "path": "user",
          "api": [
            "GET:/system/user",
            "POST:/system/user",
            "PUT:/system/user",
            "GET:/system/user/:id",
            "PUT:/system/user/:id",
            "DELETE:/system/user/:id",
            "PUT:/system/user/password/:id"
          ]
        },
        {
          "path": "config",
          "api": [
            "GET:/system/email",
            "POST:/system/email",
            "PUT:/system/email/:id",
            "GET:/system/ldap",
            "POST:/system/ldap",
            "PUT:/system/ldap/:id"
          ]
        }]
    },
    {
      "path": "external-link",
      "children": [
        {
          "path": "https://github.com/jixindatech/shenshu"
        }
      ]
    }
  ]
}